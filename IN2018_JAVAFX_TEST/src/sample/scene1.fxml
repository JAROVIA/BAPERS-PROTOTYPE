<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane fx:id="scene1" fx:controller="sample.Controller1"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Text text="Insert values in db"
          GridPane.columnIndex="1" GridPane.rowIndex="0"
          GridPane.columnSpan="2"/>

    <Label text="Table name:"
           GridPane.columnIndex="1" GridPane.rowIndex="1"/>

    <TextField fx:id="tableName"
            GridPane.columnIndex="2" GridPane.columnSpan="3" GridPane.rowIndex="1"/>

    <Label text="row1:"
           GridPane.columnIndex="1" GridPane.rowIndex="2"/>

    <TextField fx:id="row1"
            GridPane.columnIndex="2" GridPane.columnSpan="3" GridPane.rowIndex="2"/>

    <Label text="row2:"
           GridPane.columnIndex="1" GridPane.rowIndex="3"/>

    <TextField fx:id="row2"
               GridPane.columnIndex="2" GridPane.columnSpan="3" GridPane.rowIndex="3"/>

    <Label text="row3:"
           GridPane.columnIndex="1" GridPane.rowIndex="4"/>

    <TextField fx:id="row3"
               GridPane.columnIndex="2" GridPane.columnSpan="3" GridPane.rowIndex="4"/>

    <Label text="row4:"
           GridPane.columnIndex="1" GridPane.rowIndex="5"/>

    <TextField fx:id="row4"
               GridPane.columnIndex="2" GridPane.columnSpan="3" GridPane.rowIndex="5"/>

    <Button text="insert"
            onAction="#insert"
            GridPane.columnIndex="5" GridPane.rowIndex="6"/>

    <Label fx:id="notification"
           GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="7"/>

    <Button text="switch to view"
            onAction="#switchToScene"
            GridPane.columnIndex="1" GridPane.rowIndex="6"/>

</GridPane>